{

  "manifest_version": 2,
  "name": "modify-page",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "contextMenus",
    "tabs"
  ],

  "background": {
    "scripts": ["background.js", "jquery-3.2.1.min.js"]
  },

  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["jquery-3.2.1.min.js"]
  }],

  "web_accessible_resources": [
    "meme.css"
  ]

}
